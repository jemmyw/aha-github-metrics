{
  "name": "kealabs.github-metrics",
  "description": "Github Metrics",
  "version": "0.0.0",
  "author": "kealabs",
  "repository": {
    "type": "git",
    "url": "TODO: Add the GitHub URL to your extension in package.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "aha-cli": "1.9.8"
  },
  "ahaExtension": {
    "contributes": {
      "views": {
        "metricsPage": {
          "title": "Metrics Page",
          "entryPoint": "src/views/metricsPage.tsx",
          "host": "page",
          "location": {
            "menu": "Document"
          }
        }
      },
      "events": {
        "collector": {
          "title": "collector",
          "entryPoint": "src/eventHandlers/collector.ts",
          "handles": [
            "kealabs.github-metrics.webhook"
          ]
        },
        "bin": {
          "title": "bin",
          "entryPoint": "src/eventHandlers/bin.ts",
          "handles": [
            "kealabs.github-metrics.bin"
          ]
        }
      },
      "endpoints": {
        "webhook": {
          "entryPoint": "src/eventHandlers/webhook.ts",
          "public": true,
          "title": "Github metrics endpoint"
        }
      }
    }
  },
  "dependencies": {
    "recoil": "^0.3.1"
  }
}
